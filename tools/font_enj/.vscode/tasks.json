{
  "version": "2.0.0",
  "tasks": [
    {
      "type": "shell",
            "label": "C gcc build active file",
      "command": "/usr/bin/gcc",
      "args": ["-std=gnu23", "-Og", "-g", "${file}", "-o", "${fileDirname}/bin/${fileBasenameNoExtension}", "-lm"],
      "options": {
        "cwd": "${fileDirname}"
      },
      "problemMatcher": ["$gcc"],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "detail": "Task generated by Debugger."
    }
  ]
}
